<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.wfl.kits">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".overscroll.OverActivity"
            android:label="@string/action_overscroll">
            <meta-data
                android:name="moudle"
                android:value="main"/>
        </activity>
        <activity
            android:name=".materailprogress.MaterailProgressActiviy"
            android:label="@string/title_material_progress">
            <meta-data
                android:name="moudle"
                android:value="main"/>
        </activity>
        <activity android:name=".AidlActivity"/>
        <activity
            android:name=".fullscreen.FullActivity"
            android:configChanges="orientation|keyboardHidden"
            android:label="Full"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateUnchanged">
            <meta-data
                android:name="moudle"
                android:value="main"/>
        </activity>
        <activity
            android:name=".fullscreen.TranslucentActivity"
            android:screenOrientation="landscape"
            android:theme="@style/TranslucentTheme">
        </activity>
        <activity
            android:name=".vector.VectorActivity"
            android:label="Vector">
            <meta-data
                android:name="moudle"
                android:value="main"/>
        </activity>
        <activity android:name=".animateScaleType.AnimatedScaleTypeActivity"
                  android:theme="@style/AppTheme.NoActionBar"
            android:label="AnimatedScaleType">
            <meta-data
                android:name="moudle"
                android:value="main"/>
        </activity>
    </application>

</manifest>